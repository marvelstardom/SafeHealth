

<h1>data form</h1>

<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3">
        <i class="fas fa-user-plus"></i> DATA
      </h1>
      <%- include ("./partials/messages"); %>
      <form action="/users/data" method="POST">
        <div class="form-group">
          <!-- <label for="name">Name</label> -->
          <input
            type="name"
            id="name"
            name="surname"
            class="form-control"
            placeholder="Surname"
            value="<%= typeof name != 'undefined' ? name : '' %>"
          />
        </div>
        <div class=" form-group">
            <input type="text"
            id="name"
            name="othername"
            class="form-control"
            placeholder="Other Names">
        </div>

        <div class="form-group">
          <input 
          type="text"
          id="name"
          name="address"
          class="form-control"
          placeholder="Address">
        </div>
    
        <div class="form-group">
          <!-- <label for="email">Date of Birth</label> -->
          <input
            type="text"
            id="name"
            name="address"
            class="form-control"
            placeholder="Date of birth"
         
          />
        </div>
       
        <button type="submit" class="btn btn-primary btn-block">
          Save
        </button>
      </form>
      
    </div>
  </div>
</div>